#include <stdio.h>
#include <stdlib.h>
#include <string.h>
  int main()
 
{
 
 FILE *fp = fopen("CPU_BURST.txt", "r");
 int bt[10],wt[10],tat[10],i=0;
 float avg_wt,avg_tat;
 while((fgetc(fp)!EOF)
 {
    fscanf(fp,"%d",&bt[i]);
    if(bt[i]>0)
    p[i]=i+1;
    i++;
    }
    n=i;
  for(i=0;i<n-1;i++)
  {
      for(j=0;j<n-i-1;j++)
      {
         if(bt[j]>bt[j+1])
         {
            temp=bt[j];
            bt[j+1]=bt[j];
            bt[j]=temp;
            }
            }
        wt[0]=0;
        for(i=1;i<n;i++)
        {
            wt[i]=wt[i-1]+bt[i-1];
            sum_wt=sum_wt+wt[i];
           }
           avg_wt=(float(sum_wt))/((float(n));
       for(i=0;i<n;i++)
        {
            tat[i]=wt[i]+bt[i];
            sum_tat=sum_tat+tat[i];
           }
           avg_tat=(float(sum_tat))/((float(n));
